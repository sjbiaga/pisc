⟦ traces = console ⟧

⟦ exclude = yes ⟧

P(x) = (!. local x@∞ ! { 1 } /*println("out 1")*/.) | (!. local x@∞ ? { z } /*IO.pure(_).flatTap { z => IO.println(s"in $z") }*/.)

Q(x) = "l"[ !. s2s x@∞ ! { 2 } /*println("out 2")*/. ] | "r"[ !. s2s x@∞ ? { z } /*IO.pure(_).flatTap { z => IO.println(s"in $z") }*/. ]

R(x) = "p"[ "c"[ !. c2p x@∞ ! { 3 } /*println("out 3")*/. ] | ( !. p2c x@∞ ? { z } /*IO.pure(_).flatTap { z => IO.println(s"in $z") }*/. ) ]

Main = ν(x) "r"[ P(x) | Q(x) | R(x) ]

⟦ exclude = no ⟧

P = ν(x) [ "l"[ accept x /*println("accepted")*/. ] | "r"[ enter x /*println("entered")*/. ] ]

Q = ν(x) "p"[ "c"[ exit x /*println("exited")*/. ] | expel x /*println("expelled")*/. ]

R = ν(x) "r"[ "+"[ merge+ x /*println("merged+")*/. ] | "-"[ merge- x /*println("merged-")*/. ] ]

Main = P | Q | R

⟦ exclude = yes ⟧

C(x) = exit x /*println("exited")*/. s2s x?{y}. enter y /*println("entered")*/. C(y)

P(x) = expel x /*println("expelled")*/. s2s x!{νy}. accept y /*println("accepted")*/. P(y)

Main = ν(x) [ "p"[ "c"[ C(x) ] | P(x) ] ]

⟦ exclude = yes ⟧

