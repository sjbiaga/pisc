# -*- indent-tabs-mode: nil; -*-

⟦ traces = console ⟧

P = ν(x) [ "l"[ accept x. τ/*println("accepted")*/. ] | "r"[ enter x. τ/*println("entered")*/. ] ]

Q = ν(x) "p"[ "c"[ exit x. τ/*println("exited")*/. ] | expel x. τ/*println("expelled")*/. ]

R = ν(x) "r"[ "+"[ merge+ x. τ/*println("merged+")*/. ] | "-"[ merge- x. τ/*println("merged-")*/. ] ]

Main = P | Q | R
